<?xml version="1.0"?>
<block>
  <name>EQ2440animation</name>
  <key>EQ2440Module_EQ2440animation</key>
  <category>[EQ2440Module]</category>
  <import>import EQ2440Module</import>
  <make>EQ2440Module.EQ2440animation($innocence_bool)</make>
  		<callback>set_innocence_bool($innocence_bool)</callback>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>innocence_bool</name>
    <key>innocence_bool</key>
    <type>int</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>msgtrigger</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->

</block>
